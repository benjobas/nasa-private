# Puerto en el que se ejecutará el servidor
PORT=3000

# Variables relacionadas con la API de NASA POWER
NASA_POWER_API_BASE_URL=https://power.larc.nasa.gov/

#Configuracion de umbrales predeterminados
SAMPLE_SIZE=10
PROGRESS_INTERVAL=1

# Configuración de umbrales predeterminados
DEFAULT_VERY_HOT_THRESHOLD=30
DEFAULT_VERY_WINDY_THRESHOLD=8
DEFAULT_VERY_WET_THRESHOLD=10
DEFAULT_VERY_UNCOMFORTABLE_HEAT_INDEX=30
DEFAULT_VERY_UNCOMFORTABLE_HUMIDITY=70

# NASA GESDISC bearer token used for authenticated access to OPeNDAP endpoints.
# You can generate one at https://urs.earthdata.nasa.gov/profile and paste the
# value here before running the integration tests or enabling external
# observations in the forecast endpoint.
GESDISC_TOKEN=
